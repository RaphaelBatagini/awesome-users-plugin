{
    "name": "raphael-batagini/awesome-users-plugin",
    "description": "Users listing plugin.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "RaphaelBatagini\\AwesomeUsersPlugin\\": "src/",
            "RaphaelBatagini\\AwesomeUsersPlugin\\Tests\\": "tests/src/"
        }
    },
    "authors": [
        {
            "name": "Raphael Batagini"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "inpsyde/php-coding-standards": "^1.0",
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.20",
        "10up/wp_mock": "dev-trunk"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
